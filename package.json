{
  "name": "ember-can",
  "version": "3.1.0",
  "description": "Simple authorization addon for Ember apps",
  "keywords": [
    "ember-addon",
    "ember",
    "can",
    "authorization",
    "abilities"
  ],
  "repository": "https://github.com/minutebase/ember-can",
  "license": "MIT",
  "author": "Richard Livsey <richard@minutebase.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "Ben Limmer <hello@benlimmer.com>",
    "Miles Zimmerman <miles@maztek.io>",
    "Maciej Kwa≈õniak <maciej.kwasniak@macsour.com>"
  ],
  "scripts": {
    "changelog": "lerna-changelog",
    "release": "release-it",
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags --no-verify"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  },
  "dependencies": {
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.2.1",
    "ember-inflector": "^4.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "1.3.0",
    "@glimmer/component": "1.0.0",
    "babel-eslint": "10.0.3",
    "broccoli-asset-rev": "3.0.0",
    "ember-auto-import": "1.5.3",
    "ember-cli": "~3.15.1",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-inject-live-reload": "2.0.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.1",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.6.0",
    "ember-resolver": "7.0.0",
    "ember-source": "~3.15.0",
    "ember-source-channel-url": "2.0.1",
    "ember-template-lint": "1.13.1",
    "ember-try": "1.4.0",
    "eslint": "6.8.0",
    "eslint-plugin-ember": "7.7.2",
    "eslint-plugin-node": "11.0.0",
    "lerna-changelog": "1.0.0",
    "loader.js": "4.7.0",
    "qunit-dom": "0.9.2",
    "release-it": "12.4.3",
    "release-it-lerna-changelog": "1.0.3"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "changelog": {
    "repo": "minutebase/ember-can",
    "labels": {
      "breaking": ":boom: Breaking Change",
      "enhancement": ":rocket: Enhancement",
      "bug": ":bug: Bug Fix",
      "documentation": ":memo: Documentation",
      "internal": ":house: Internal"
    }
  }
}
